}

widget = {
	name = "portrait_editor_window"
	size = { 1635 1000 }
	filter_mouse = all
	layoutpolicy_horizontal = expanding
	layoutpolicy_vertical = expanding

	vbox = {
		pe_tab_buttons_hbox = {
			layoutpolicy_horizontal = expanding
			name = "tab_buttons"
		}

		pe_editor_tab = {
			layoutpolicy_horizontal = expanding
			layoutpolicy_vertical = expanding
			name = "editor_tab"
			visible = "[PortraitEditorWindow.IsCurrentContentTab('editor_tab')]"
		}

		pe_preview_tab1 = {
			layoutpolicy_horizontal = expanding
			layoutpolicy_vertical = expanding
			name = "preview_tab1"
			visible = "[PortraitEditorWindow.IsCurrentContentTab('preview_tab1')]"
		}

		pe_preview_tab2 = {
			layoutpolicy_horizontal = expanding
			layoutpolicy_vertical = expanding
			name = "preview_tab2"
			visible = "[PortraitEditorWindow.IsCurrentContentTab('preview_tab2')]"
		}

		pe_children_tab = {
			layoutpolicy_horizontal = expanding
			layoutpolicy_vertical = expanding
			name = "children_tab"
			visible = "[PortraitEditorWindow.IsCurrentContentTab('children_tab')]"
		}
	}
}

widget = {
	name = "portrait_3dview_window"
	size = { 840 920 }

	layoutpolicy_horizontal = expanding
	layoutpolicy_vertical = expanding

	portrait_preview_widget = {
		size = { 200% 200% }	# Increase render resolution of portrait
		scale = 0.5

		name = "3d_preview"
		entity_instance = "[Portrait3dView.GetPortraitEntity('(int32)0')]"
		portrait_context = "[Portrait3dView.GetPortraitContext]"
		camera_look_at = { 0 120 -2 }
		camera_position = { -120 150 -300 }
		camera_zoom_limits = { 0.1 5 }
		camera_near_far = { 0.5 2000 }
		camera_fov_y_degrees = 20
		raw_tooltip = "[Select_CString( ShouldShowAnimationInfo, Portrait3dView.GetAnimationTooltip, '' )]"
	}

	pe_tab_button = {
		raw_text =  "Save Screenhot"
		size = { 120 50 }
		position = { 0 1 }
		onclick = "[Portrait3dView.SaveScreenshot]"
	}
}

types PortraitEditorTypes
{
	# Widgets
	type pe_header_textbox = textbox {
		autoresize = yes
		align = nobaseline
		fontsize = 16
		using = editor_font
	}

	type pe_sub_header_textbox = textbox {
		autoresize = yes
		fontsize = 14
		using = editor_font
	}

	type aligned_left_widget = hbox {

		layoutpolicy_horizontal = expanding
		block "content"
		{

		}

		widget = {
			layoutpolicy_horizontal = expanding
			name = "spacer"
		}
	}

	type aligned_left_widget_group = hbox {
		layoutpolicy_horizontal = expanding
		margin_bottom = 4

		background = {
			using = pe_background_outline
		}

		vbox = {
			block "group_content"
			{

			}
		}

		widget = {
			layoutpolicy_horizontal = expanding
		}
	}

	type aligned_left_widget_subgroup = hbox {
		layoutpolicy_horizontal = expanding
		margin = { 2 2 }

		vbox = {
			block "group_content"
			{

			}
		}

		widget = {
			layoutpolicy_horizontal = expanding
		}
	}

	type pe_margin_widget = widget {
		layoutpolicy_horizontal = expanding
	}

	type pe_select_box = hbox {
		margin_right = 16
	}

	type pe_button_right = button {
		using = pe_button
		using = editor_font
		size = { 22 22 }
		raw_text =  ">"
		align = center|nobaseline
	}

	type pe_button_left = button {
		using = pe_button
		using = editor_font
		size = { 22 22 }
		raw_text =  "<"
		align = center|nobaseline
	}

	type pe_button_open_file = button {
		using = pe_button
		using = editor_font
		size = { 38 22 }
		raw_text =  "Open"
		align = center|nobaseline
	}

	type pe_editor_tab = hbox {

		pe_editor_tab_settings = {
			layoutpolicy_vertical = expanding

		}

		editor_scrollarea = {
			layoutpolicy_horizontal = expanding
			layoutpolicy_vertical = expanding

			blockoverride "content"
			{
				fixedgridbox = {
					datamodel = "[PortraitEditorWindow.GetActiveTypes]"
					addrow = 420
					addcolumn = 520
					datamodel_wrap = 2
					flipdirection = yes

					item = {
						pe_editor_tab_portraits = {
							blockoverride "label_text" { text = "[PortraitType.GetKey]" }
							blockoverride "portrait_texture"
							{
								portrait_texture = "[PortraitEditorWindow.GetPortraitTexture( PortraitType.Self )]"
								onclick = "[PortraitEditorWindow.Open3dView( PortraitType.Self )]"
								raw_tooltip = "Click to open 3D view [Select_CString( ShouldShowAnimationInfo, PortraitEditorWindow.GetAnimationTooltip( PortraitType.Self ), '' )]"
							}
							blockoverride "open_preview" { onclick = "[PortraitEditorWindow.Open3dView( PortraitType.Self )]" }
							blockoverride "copy_persistent_info" { onclick = "[PortraitEditorWindow.OnCopyPersistentPortraitInfo( PortraitType.Self )]" }
						}
					}
				}
			}
		}

		pe_editor_tab_reload = {
			layoutpolicy_vertical = expanding
		}
	}

	# Settings panel
	type pe_editor_tab_settings = vbox {
		layoutpolicy_horizontal = fixed
		minimumsize = { 340 -1 }
		margin_left = 2
		margin = { 5 5 }
		spacing = 1

		background = {
			using = pe_background_simple
		}

		# Header
		aligned_left_widget = {
			blockoverride "content"
			{
				textbox = {
					autoresize = yes
					align = left|nobaseline
					using = editor_font
					fontsize = 25
					raw_text =  "Portrait Editor"
				}
			}
		}

		# Portrait type group
		aligned_left_widget_group = {
			blockoverride "group_content"
			{
				aligned_left_widget = {
					blockoverride "content"
					{
						pe_header_textbox = {
							raw_text =  "Portrait Type Group:"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						pe_button_left = {
							onclick = "[PortraitEditorWindow.OnCyclePortraitTypeGroup( '(int32)-1' )]"
						}

						pe_select_box = {
							name = "portrait_type_group_list"
						}

						pe_button_right = {
							onclick = "[PortraitEditorWindow.OnCyclePortraitTypeGroup( '(int32)1' )]"
						}

						widget = {
							size = { 8 0 }
						}

						pe_button_open_file = {
							tooltip = "[PortraitEditorWindow.GetCurrentPortraitTypeGroupFileDesc]"
							onclick = "[PortraitEditorWindow.OpenCurrentPortraitTypeGroupFile]"
						}
					}
				}
			}
		}

		# Animations
		aligned_left_widget_group = {
			blockoverride "group_content"
			{
				aligned_left_widget = {
					blockoverride "content"
					{
						pe_header_textbox = {
							raw_text =  "Animations:"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						pe_button_left = {
							onclick = "[PortraitEditorWindow.OnChangeAnimationItemLeft]"
						}

						pe_select_box = {
							name = "animations_list"
						}

						pe_button_right = {
							onclick = "[PortraitEditorWindow.OnChangeAnimationItemRight]"
						}

						widget = {
							size = { 8 0 }
						}

						pe_button_open_file = {
							tooltip = "[PortraitEditorWindow.GetCurrentAnimationFileDesc]"
							onclick = "[PortraitEditorWindow.OpenCurrentAnimationFile]"
						}
					}
				}
			}
		}

		# Ethnicity
		aligned_left_widget_group = {
			blockoverride "group_content"
			{
				aligned_left_widget = {
					blockoverride "content"
					{
						pe_header_textbox = {
							raw_text =  "Ethnicity:"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						pe_button_left = {
							onclick = "[PortraitEditorWindow.OnCycleEthnicity( '(int32)-1' )]"
						}

						pe_select_box = {
							name = "ethnicity_list"
						}

						pe_button_right = {
							onclick = "[PortraitEditorWindow.OnCycleEthnicity( '(int32)1' )]"
						}

						widget = {
							size = { 8 0 }
						}

						pe_button_open_file = {
							tooltip = "[PortraitEditorWindow.GetCurrentEthnicityFileDesc]"
							onclick = "[PortraitEditorWindow.OpenCurrentEthnicityFile]"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						flowcontainer = {
							direction = vertical
							spacing = 1
							margin = { 3 3 }
							margin_left = 38

							button = {
								using = pe_button
								using = editor_font
								size = { 120 20 }
								fontsize = 12
								raw_text = "Randomize DNA"
								onclick = "[PortraitEditorWindow.OnGenerateFromEthnicity]"
							}

							button = {
								using = pe_button
								using = editor_font
								size = { 180 20 }
								fontsize = 12
								raw_text = "Randomize except template"
								onclick = "[PortraitEditorWindow.OnGenerateFromEthnicityExcept]"
							}

							button = {
								using = pe_button
								using = editor_font
								size = { 140 20 }
								fontsize = 12
								raw_text = "Reset DNA to neutral"
								onclick = "[PortraitEditorWindow.OnResetDNA]"
							}
						}
					}
				}
			}
		}

		# Age
		aligned_left_widget_group = {
			blockoverride "group_content"
			{
				spacing = 5
				aligned_left_widget = {
					blockoverride "content"
					{
						textbox = {
							size = { 150 25 }
							fontsize = 16
							using = editor_font
							raw_text =  "Age: [PortraitEditorWindow.GetAge]%"
						}
					}
				}

				scrollbar = {
					name = "age_scrollbar"
					size = { 270 14 }
					direction = horizontal
					min = 0
					max = 1
					step = 0.005
					wheelstep = 0.005
					value = "[PortraitEditorWindow.GetAgePercent]"
					onvaluechanged = "[PortraitEditorWindow.OnAgeChanged]"

					track = {
						using = editor_scrollbar_track
					}

					slider = {
						using = editor_scrollbar_slider
					}

					dec_button = {
						button = {
							using = editor_scrollbar_button
							upframe = 9
							downframe = 10
							overframe = 11
							disableframe = 12
						}
					}

					inc_button = {
						button = {
							using = editor_scrollbar_button
							upframe = 13
							downframe = 14
							overframe = 15
							disableframe = 16
						}
					}
				}

			}
		}

		# Environment
		aligned_left_widget_group = {
			blockoverride "group_content"
			{
				aligned_left_widget = {
					blockoverride "content"
					{
						pe_header_textbox = {
							raw_text =  "Environment:"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						pe_button_left = {
							onclick = "[PortraitEditorWindow.OnPreviousEnvironment]"
						}

						pe_select_box = {
							name = "environment_list"
						}

						pe_button_right = {
							onclick = "[PortraitEditorWindow.OnNextEnvironment]"
						}

						widget = {
							size = { 8 0 }
						}

						pe_button_open_file = {
							tooltip = "[PortraitEditorWindow.GetCurrentEnvironmentFileDesc]"
							onclick = "[PortraitEditorWindow.OpenCurrentEnvironmentFile]"
						}
					}
				}
			}
		}

		# Camera
		aligned_left_widget_group = {
			blockoverride "group_content"
			{
				aligned_left_widget = {
					blockoverride "content"
					{
						pe_header_textbox = {
							raw_text =  "Camera:"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						pe_button_left = {
							onclick = "[PortraitEditorWindow.OnPreviousCamera]"
						}

						pe_select_box = {
							name = "camera_list"
						}

						pe_button_right = {
							onclick = "[PortraitEditorWindow.OnNextCamera]"
						}

						widget = {
							size = { 8 0 }
						}

						pe_button_open_file = {
							tooltip = "[PortraitEditorWindow.GetCurrentCameraFileDesc]"
							onclick = "[PortraitEditorWindow.OpenCurrentCameraFile]"
						}
					}
				}
			}
		}

		# Gene Extras
		aligned_left_widget_group = {
			layoutpolicy_vertical = expanding
			layoutpolicy_horizontal = expanding

			blockoverride "group_content"
			{
				spacing = 5
				layoutpolicy_vertical = expanding
				layoutpolicy_horizontal = expanding

				aligned_left_widget = {
					blockoverride "content"
					{
						pe_header_textbox = {
							raw_text =  "Gene:"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						pe_button_left = {
							onclick = "[PortraitEditorWindow.OnChangeGeneLeft]"
						}

						pe_select_box = {
							name = "gene_list"
						}

						pe_button_right = {
							onclick = "[PortraitEditorWindow.OnChangeGeneRight]"
						}

						widget = {
							size = { 8 0 }
						}

						pe_button_open_file = {
							tooltip = "[PortraitEditorWindow.GetCurrentGeneFileDesc]"
							onclick = "[PortraitEditorWindow.OpenCurrentGeneFile]"
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						flowcontainer = {
							direction = vertical
							spacing = 4

							pe_sub_header_textbox = {
								raw_text =  "Gene template"
								visible = "[PortraitEditorWindow.IsSelectedMorphGene]"
							}

							pe_sub_header_textbox = {
								raw_text =  "Sub Group"
								visible = "[PortraitEditorWindow.IsSelectedAccessoryGene]"
							}

							flowcontainer = {
								visible = "[Not(PortraitEditorWindow.IsSelectedColorGene)]"

								pe_button_left = {
									onclick = "[PortraitEditorWindow.OnChangeSubGeneItemLeft]"
								}

								widget = { size = { 16 0 } }

								dropDown = {
									alwaystransparent = no
									name = "gene_templates_dropdown"
									size = { 192 20 }

									active_item = {
										hbox = {
											alwaystransparent = no
											button = {
												layoutpolicy_horizontal = expanding
												layoutpolicy_vertical = expanding
												texture = "gfx/editor_gui/editor_dropdown.dds"
												spriteType = CorneredStretched
												gfxtype = framedbuttongfx
												effectname = "NoHighlight"
												spriteborder = { 4 4 }
												spriteborder_right = 24
												button_trigger = none
												framesize = { 32 16 }
												upframe = 1
												downframe = 2
												overframe = 3
												disableframe = 4

												pe_text = {
													visible = "[PortraitEditorWindow.HasSubGenes]"
													margin_top = 1
													margin_right = 16
													parentanchor = hcenter
													align = hcenter
													autoresize = yes
													fontsize = 10
													text = "[GeneItem.GetName]"
												}
											}
										}
									}

									item = {
										widget = {
											alwaystransparent = no
											size = { 280 20 }

											button = {
												size = { 280 18 }
												texture = "gfx/editor_gui/editor_dropdown_item.dds"
												spriteType = CorneredTiled
												spriteborder = { 1 1 }
												gfxtype = framedbuttongfx
												framesize = { 8 8 }
												upframe = 1
												downframe = 2
												overframe = 2
												disableframe = 1
												button_trigger = none
											}

											pe_text = {
												text = "[GeneItem.GetName]"
												position = { 10 0 }
												size = { 280 18 }
											}
										}
									}

									list = {
										scrollarea = {
											position = { 2 20 }
											maximumsize = { 280 510 }
											autoresizescrollarea = yes

											background = {
												margin = { 2 2 }
												margin_bottom = 6
												margin_right = 6
												texture = "gfx/editor_gui/editor_dropdown_list.dds"
												spriteType = CorneredStretched
												spriteborder = { 6 6 }
												spriteborder_right = 8
												spriteborder_bottom = 8
												shaderfile = "gfx/FX/pdxgui_default.shader"
											}

											scrollwidget = {
												fixedgridbox = {
													alwaystransparent = no
													addcolumn = 275
													addrow = 18
												}
											}

											scrollbar_vertical = {
												using = editor_vertical_scrollbar
											}

											scrollbar_horizontal = {
												using = editor_horizontal_scrollbar
											}
										}
									}
								}

								widget = { size = { 16 0 } }

								pe_button_right = {
									onclick = "[PortraitEditorWindow.OnChangeSubGeneItemRight]"
								}
							}
						}
					}
				}

				# Gene strength/value
				aligned_left_widget_subgroup = {
					visible = "[Not(PortraitEditorWindow.IsSelectedColorGene)]"

					blockoverride "group_content"
					{
						spacing = 4
						aligned_left_widget = {
							blockoverride "content"
							{
								textbox = {
									autoresize = yes
									fontsize = 13
									using = editor_font
									raw_text =  "Strength: [PortraitEditorWindow.GetSelectedGeneTemplateStrength|%1]"
								}
							}
						}

						scrollbar = {
							name = "strength_scrollbar"
							size = { 265 14 }

							direction = horizontal
							min = 0
							max = 1
							step = 0.005
							wheelstep = 0.005

							track = {
								using = editor_scrollbar_track
							}

							slider = {
								using = editor_scrollbar_slider
							}

							dec_button = {
								button = {
									using = editor_scrollbar_button
									upframe = 9
									downframe = 10
									overframe = 11
									disableframe = 12
								}
							}

							inc_button = {
								button = {
									using = editor_scrollbar_button
									upframe = 13
									downframe = 14
									overframe = 15
									disableframe = 16
								}
							}
						}
					}
				}

				aligned_left_widget = {
					blockoverride "content"
					{
						textbox = {
							autoresize = yes
							minimumsize = { -1 70 }
							using = editor_font
							fontsize = 11
							text = "[PortraitEditorWindow.GetCurrentGeneAccessoryString]"
							visible = "[PortraitEditorWindow.IsSelectedAccessoryGene]"
						}
					}
				}

				# Color Coordinates
				aligned_left_widget = {
					blockoverride "content"
					{
						flowcontainer = {
							direction = vertical
							visible = "[PortraitEditorWindow.IsSelectedColorGene]"

							textbox = {
								size = { 150 20 }
								fontsize = 14
								using = editor_font
								raw_text =  "Color Coordinates"
							}

							textbox = {
								size = { 150 20 }
								fontsize = 13
								using = editor_font
								raw_text =  "X: [PortraitEditorWindow.GetSelectedGeneTemplateColorX|%1]"
							}

							editor_slider = {
								name = "colorx_scrollbar"
								size = { 270 14 }
								direction = horizontal
								min = 0
								max = 1
								step = 0.005
								wheelstep = 0.005
								tracknavigation = direct
							}

