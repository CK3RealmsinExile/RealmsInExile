template pe_slider_button
{
	using = pe_button
	fontsize = 12
	size = { 60 15 }
	parentanchor = top|left
	layoutpolicy_horizontal = Growing
}

template pe_slider_checkbox_text
{
	size = { 20 15 }
	fontsize = 13
	using = editor_font
	align = left|nobaseline
	layoutpolicy_horizontal = Growing
	raw_text =  "_"
	autoresize = yes
}

template pe_background_simple
{
	texture = "gfx/backgrounds/bg_outline_simple.dds"
	color = { 0.1 0.1 0.1 1 }
	spriteType = Corneredtiled
	spriteborder = { 10 10 }
	texture_density = 4

}

