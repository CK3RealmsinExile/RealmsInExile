template pe_color_1
{
	color = { 0.5 0.7 0.9 1.0 }
}
template pe_button
{
	using = editor_button
	using = pe_color_1
}

template pe_torso_button_template
{
	pop_out = no
	gfxtype = portraitbuttongfx
	shaderfile = "gfx/FX/jomini/gui_portrait.shader"

	intersectionmask = no

	background_texture = "gfx/portraits/portrait_transparent.dds"
	texture = "gfx/portraits/portrait_transparent.dds"

	block "portrait_transformation"
	{
		portrait_scale = { 1.0 1.0 }
		portrait_offset = { 0.0 0.0 }
	}
}

template pe_standard_full_body
{
	using = pe_torso_button_template
	alwaystransparent = no
	size = { 276 418 }

	#portrait_texture = "[Character.GetPortrait('full_body', 'full_body')]"
